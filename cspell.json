{
    "version": "0.2",
    "language": "en,zh-CN",
    "dictionaries": ["typescript", "node", "npm", "python", "bash", "software-terms"],
    "dictionaryDefinitions": [
        {
            "name": "custom-words",
            "path": "./cspell-custom-words.txt",
            "addWords": true
        }
    ],
    "words": [
        "aiakali",
        "AIAkali",
        "aiohappyeyeballs",
        "Akali",
        "commitizen",
        "decli",
        "lfmh",
        "nonebot",
        "onebot",
        "precommit",
        "pyproject"
    ],
    "ignorePaths": [
        "node_modules/**",
        ".bot/**",
        "**/__pycache__/**",
        "**/*.pyc",
        "bun.lock",
        "*.lock",
        ".git/**",
        "dist/**",
        "build/**",
        "requirements.txt",
        "cspell-custom-words.txt"
    ],
    "flagWords": [],
    "languageSettings": [
        {
            "languageId": "*",
            "caseSensitive": false
        },
        {
            "languageId": "python",
            "ignoreRegExpList": ["/#.*/g"]
        },
        {
            "languageId": "typescript",
            "ignoreRegExpList": ["/import.*from.*['\"].*/g"]
        }
    ]
}
